<%= simple_form_for(@bunker) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user %>
    <%= f.input :price %>
    <%= f.association :location %>
    <%= f.association :review %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
